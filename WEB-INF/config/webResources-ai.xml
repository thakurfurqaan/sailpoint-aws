<sailpoint>
    
    <ImportAction name="merge">
      <Configuration name="WebResource">
        <Attributes>
          <Map>
            <entry key="webResources">
              <value>
                <List>
                    <!--
                      IAI Configuration
                     -->
                    <WebResource rights="FullAccessIAIConfiguration" url="systemSetup/index.*">
                      <ChildResources>
                        <String>systemSetup/iaiConfig.*</String>
                      </ChildResources>
                    </WebResource>
                    <WebResource rights="FullAccessIAIConfiguration" url="systemSetup/iaiConfig.*"/>
                </List>
              </value>
            </entry>
          </Map>
        </Attributes>
      </Configuration>
    </ImportAction>
</sailpoint>